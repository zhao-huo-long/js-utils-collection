{
  "buildOptions": {},
  "compilerOptions": {
    "allowJs": true,
    "target": "ES6",
    "noUnusedLocals": false,
    "noEmitOnError": true,
    "allowUnreachableCode": false,
    "module": "ES6",
    "alwaysStrict": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "moduleResolution": "node",
    "declaration": true,
    "strict": true,
    "strictNullChecks": true
  },
  "include": [
    "src/**/*.ts"
  ],
}
