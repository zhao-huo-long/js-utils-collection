{
  "name": "js-utils-collection",
  "version": "1.0.0",
  "description": "js-utils-collection",
  "main": "dist/src/index.js",
  "module": "esm/src/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"skip test\" ",
    "clean": "rimraf dist && rimraf esm",
    "build": "npm run clean && npm run build-common && npm run build-es && npm run copy",
    "copy": "cp src/types.d.ts dist && cp src/types.d.ts esm",
    "build-common": "cross-env module_type=cjs rollup --config",
    "build-es": "cross-env module_type=esm rollup --config",
    "lint": "npx eslint src/*",
    "lint-fix": "npx eslint  --fix src/*",
    "prettier": "npx prettier --write src/*",
    "playground": "pnpm run build && cd playground/js-utils-collection-playground && pnpm i && pnpm link ../.. && pnpm run dev",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,css,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhao-huo-long/js-utils-collection.git"
  },
  "keywords": [
    "js-utils-collection"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhao-huo-long/js-utils-collection/issues"
  },
  "homepage": "https://github.com/zhao-huo-long/js-utils-collection#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "2.8.4",
    "rollup": "^3.18.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "rimraf": "^4.3.1"
  },
  "dependencies": {}
}
